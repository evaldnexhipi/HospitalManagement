    <script>
        $(document).ready(function() {
            var myChart = document.getElementById("revenuesChartCanvas").getContext('2d');

            $.get('http://localhost:8000/getLastReservationsFreq', function (data) {
                var months = new Array();
                var counts = new Array();
                for (var i=0; i<data.length; i++){
                    months.push(data[i].month);
                    counts.push(data[i].count);
                }

                var massPopChart = new Chart(myChart,{
                    type: 'line', //bar,horizontalBar, pie, line, doughnut, radar, polarArea
                    data: {
                        labels: months,
                        datasets: [{
                            label: 'Muajt',
                            data:counts,
                            backgroundColor: 'rgba(238, 238, 0, 1)'
                        }] //can have more than one data set
                    },
                    options: {
                        legend: {
                            display: false
                        },
                        scales: {
                            xAxes: [{
                                gridLines: {
                                    display: false,
                                }
                            }],
                            yAxes: [{
                                // display: false,
                                gridLines: {
                                    display: false,
                                }
                            }],
                        }
                    }
                });
            });

            // $("#revenuesChart").html(myChart.generateLegend());
        });
